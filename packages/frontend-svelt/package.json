{
  "name": "@opendata/frontend",
  "version": "0.0.1",
  "scripts": {
    "start": "yarn run dev",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "lint:check": "eslint --fix '{,src/**/}*.{js,ts,svelte}' && prettier --write '{,src/**/}*.{js,ts,svelte,json,md}'",
    "lint:fix": "eslint --fix '{,src/**/}*.{js,ts,svelte}' && prettier --write '{,src/**/}*.{js,ts,svelte,json,md}'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "@urql/svelte": "^4.0.4"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1",
    "@iconify-json/icon-park": "^1",
    "@iconify-json/line-md": "^1",
    "@iconify-json/mdi": "^1",
    "@iconify-json/svg-spinners": "^1",
    "@iconify/types": "^2.0.0",
    "@neoconfetti/svelte": "^1.0.0",
    "@playwright/test": "^1.28.1",
    "@popperjs/core": "^2.11.8",
    "@supabase/ssr": "^0.0.5",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1",
    "@types/cookie": "^0.5.1",
    "@types/lodash-es": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@urql/core": "^4.1.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.34.0",
    "graphql": "^16.8.1",
    "lodash-es": "^4.17.21",
    "marked": "^9.1.4",
    "postcss": "^8.4.31",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.2.2",
    "svelte-apollo": "^0.5.0",
    "svelte-check": "^3.5.2",
    "svelte-motion": "^0.11.5",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "unplugin-icons": "^0.17.1",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "volta": {
    "node": "18.18.0",
    "yarn": "3.6.4"
  }
}
